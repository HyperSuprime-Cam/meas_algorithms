#<? cfg paf dictionary ?>
#
# 
target: PsfSelectionRhl

definitions: {
    fluxLim: {
        type: "double"
        description: "specify the minimum psfFlux for good Psf Candidates"
        minOccurs: 0
        maxOccurs: 1
        default: 1000.0
        allowed: {
            min: 0.0
        }
    }
    clumpNSigma: {
        type: "double"
        description: "Candidate PSF's shapes must lie within this many sigma of the average shape"
        minOccurs: 0
        maxOccurs: 1
        default: 1.0
        allowed: {
            min: 0.0
        }
    }
    kernelSize: {
        type: "int"
        description: "size of the kernel to create"
        minOccurs: 1
        maxOccurs: 1
        default: 21
    }
    borderWidth: {
        type: "int"
        description: "Number of pixels to ignore around the edge of PSF candidate postage stamps"
        minOccurs: 1
        maxOccurs: 1
        default: 0
    }
    
    sizeCellX: {
        type: "int"
        description: "size of cell used to determine PSF. column direction"
        minOccurs: 1
        maxOccurs: 1
        allowed: {
            min: 10
        }
        default: 256
    }
    sizeCellY: {
        type: "int"
        description: "size of cell used to determine PSF. column direction"
        minOccurs: 1
        maxOccurs: 1
        allowed: {
            min: 10
        }
        default: 256
    }
}
